// This test checks whether empty reduces are handled properly.
fun {bool,int} main() =
  let n = 0 in
  let {a,b} = reduce(fn {bool,int} ({bool,int} acc, {bool,int} elem) =>
                       let {accx, accy} = acc in
                       let {x, y} = elem in
                       {accx && x,
                        y},
                     {False,0}, zip(replicate(n,True), replicate(n,1))) in
  {a,b}
