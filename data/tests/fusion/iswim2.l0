fun {[[real]]} main(*[real] row, *[[real]] arr) =
  let {arr2} =
    scanT(fn {*[real]} ([real] a, [real] b) =>
            let {x} =
              mapT(fn {real} (real x, real y) =>
                     {x * y},
                   a, b) in
            {x},
          {row}, arr) in
  mapT(fn {[real]} ([real] r) => mapT(fn {real} (real x) => {x + 2.0}, r), arr2)
