fun main(rss: [k][n][m]int): [][]int =
  map(fn (rs: [][]int): [n]int  =>
        map(fn (r: []int): int  =>
              loop (x = 0) = for i < m do
                x * 2 + r[i]
              in x,
            rs),
      rss)
